    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>7fd7753f-4871-4909-98a8-5710b019d664</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.9" />
